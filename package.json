    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "electron": "^13.1.5",
    "eslint": "^7.28.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "bootstrap": "^5.0.2",
    "bugsplat-node": "^2.0.1"
  }
